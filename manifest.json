{
  "manifest_version": 3,
  "name": "LeetCode to Russian",
  "description": "Translate LeetCode Problems to Russian",
  "version": "0.0.7",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "js": ["script.js"],
      "matches": [
        "https://*.leetcode.com/problems/*"
      ]
    }
  ],
  "permissions": ["storage"]
}