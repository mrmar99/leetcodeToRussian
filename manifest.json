{
  "manifest_version": 3,
  "name": "LeetCode to Russian",
  "description": "Translate LeetCode Problems to Russian",
  "version": "0.0.6",
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "content_scripts": [
    {
      "js": ["script.js"],
      "matches": [
        "https://*.leetcode.com/problems/*"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [{ 
  "resources": ["data/translations.json", "data/keywords.json"],
  "matches": ["<all_urls>"]
}]
}